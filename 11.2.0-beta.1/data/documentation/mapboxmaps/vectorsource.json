udgetSize":{"role":"symbol","kind":"symbol","navigatorTitle":[{"text":"TileCacheBudgetSize","kind":"identifier"}],"identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/TileCacheBudgetSize","fragments":[{"text":"enum","kind":"keyword"},{"kind":"text","text":" "},{"text":"TileCacheBudgetSize","kind":"identifier"}],"title":"TileCacheBudgetSize","abstract":[{"type":"text","text":"Defines a TileCacheBudgetSize, which can be set as either a megabyte or tile count limit."},{"type":"text","text":" "},{"text":"Whenever tile cache goes over the defined limit","type":"text"},{"text":" ","type":"text"},{"text":"the least recently used tile will be evicted from the in-memory cache","type":"text"}],"type":"topic","url":"\/documentation\/mapboxmaps\/tilecachebudgetsize"},"doc://MapboxMaps/documentation/MapboxMaps/RasterSource":{"abstract":[{"type":"text","text":"A raster tile source."}],"type":"topic","kind":"symbol","identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/RasterSource","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"RasterSource"}],"navigatorTitle":[{"kind":"identifier","text":"RasterSource"}],"url":"\/documentation\/mapboxmaps\/rastersource","title":"RasterSource","role":"symbol"},"doc://MapboxMaps/documentation/MapboxMaps/VectorSource/id":{"url":"\/documentation\/mapboxmaps\/vectorsource\/id","fragments":[{"kind":"keyword","text":"let"},{"text":" ","kind":"text"},{"text":"id","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:SS","kind":"typeIdentifier","text":"String"}],"identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/VectorSource\/id","abstract":[],"title":"id","kind":"symbol","role":"symbol","type":"topic"},"doc://MapboxMaps/documentation/MapboxMaps/VectorSource/tileRequestsDelay":{"title":"tileRequestsDelay","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"tileRequestsDelay"},{"text":": ","kind":"text"},{"text":"Double","kind":"typeIdentifier","preciseIdentifier":"s:Sd"},{"kind":"text","text":"?"}],"identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/VectorSource\/tileRequestsDelay","url":"\/documentation\/mapboxmaps\/vectorsource\/tilerequestsdelay","abstract":[{"type":"text","text":"For the tiled sources, this property sets the tile requests delay. The given delay comes in action only during an ongoing animation or gestures. It helps to avoid loading, parsing and rendering of the transient tiles and thus to improve the rendering performance, especially on low-end devices."}],"role":"symbol","type":"topic","kind":"symbol"},"doc://MapboxMaps/documentation/MapboxMaps/VectorSource/attribution":{"url":"\/documentation\/mapboxmaps\/vectorsource\/attribution","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"attribution"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:SS","text":"String","kind":"typeIdentifier"},{"text":"?","kind":"text"}],"abstract":[{"type":"text","text":"Contains an attribution to be displayed when the map is shown to a user."}],"kind":"symbol","type":"topic","title":"attribution","identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/VectorSource\/attribution"},"doc://MapboxMaps/documentation/MapboxMaps/VectorSource/prefetchZoomDelta":{"role":"symbol","abstract":[{"text":"When loading a map, if PrefetchZoomDelta is set to any number greater than 0, the map will first request a tile at zoom level lower than zoom - delta, but so that the zoom level is multiple of delta, in an attempt to display a full map at lower resolution as quick as possible. It will get clamped at the tile source minimum zoom. The default delta is 4.","type":"text"}],"title":"prefetchZoomDelta","type":"topic","url":"\/documentation\/mapboxmaps\/vectorsource\/prefetchzoomdelta","kind":"symbol","identifier":"doc:\/\/MapboxMaps\/documentation\/MapboxMaps\/VectorSource\/prefetchZoomDelta","fragments":[